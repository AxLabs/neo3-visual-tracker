{
  "name": "neo3-visual-tracker",
  "publisher": "ngd-seattle",
  "displayName": "Neo 3 Visual DevTracker",
  "description": "A Neo Express blockchain explorer that is directly available within Visual Studio Code",
  "version": "0.0.1",
  "icon": "resources/neo-logo.png",
  "galleryBanner": {
    "color": "#242424",
    "theme": "dark"
  },
  "author": "Neo Global Development Seattle",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "bugs": {
    "url": "https://github.com/ngdseattle/neo3-visual-tracker/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngdseattle/neo3-visual-tracker.git"
  },
  "activationEvents": [
    "onCommand:neo3-visual-tracker.helloWorld"
  ],
  "keywords": [
    "Neo",
    "Blockchain"
  ],
  "main": "./dist/extension/index.js",
  "contributes": {
    "commands": [
      {
        "command": "neo3-visual-tracker.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "compile": "webpack --config src/extension/webpack.config.js --mode development",
    "package": "webpack --config src/extension/webpack.config.js --mode production && vsce package",
    "setversion": "gulp setversion",
    "watch": "webpack --config src/extension/webpack.config.js --mode development --watch --info-verbosity verbose"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/gulp": "^4.0.6",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/vscode": "^1.46.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "nerdbank-gitversioning": "^3.2.7-beta",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "vsce": "^1.77.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {}
}
